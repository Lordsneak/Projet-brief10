    <div class="position-c">
      <section class="container-fluid ">
        <section class="row justify-content-center">
          <section class="col-sm-12 col-md-4">
            <H1 class="h1class"> Add Fournisseur</H1>
            <form class="form-container" action="/fournisseur" method="POST">
              <div class="form-group">
                <label>Name</label>
                <input type="text" class="form-control" aria-label="" name="name">
              </div>
              <div class="form-group">
                <label>Email</label>
                <input type="text" class="form-control" aria-label="" name="email">
              </div>
              <div class="form-group">
                <label>Telephone</label>
                <input type="text" class="form-control" aria-label="" name="telephone">
              </div>
              <div class="form-group">
                <label>Adresse</label>
                <input type="text" class="form-control" aria-label="" name="adresse">
              </div>
              <button type="submit" class="btn btn-primary btn-block">Submit</button>
            </form>
          </section>
          <section class="col-sm-12 col-md-4">
            <H1 class="h1class">Add Rayon</H1>
            <form class="form-container" action="/rayon" method="POST">
              <div class="form-group">
                <label>Category</label>
                <input type="text" class="form-control" aria-label="" name="category">
              </div>
              <div class="form-group">
                <label>Info-Rayon</label>
                <input type="text" class="form-control" aria-label="" name="info_rayon">
              </div>
  
              <button type="submit" class="btn btn-primary btn-block">Submit</button>
            </form>
          </section>
          <section class="col-sm-12 col-md-4">
            <H1 class="h1class">Add Produit</H1>
            <form class="form-container" action="/produit" method="POST">
              <div class="form-group">
                <label>Name</label>
                <input type="text" class="form-control" aria-label="insert Name" name="name">
              </div>
              <div class="form-group">
                <label>Image</label>
                <input type="text" class="form-control" aria-label="Link Source" name="image">
              </div>
              <div class="form-group">
                <label>Quantite</label>
                <input type="text" class="form-control" aria-label="nom Auteur" name="quantite">
              </div>
              <div class="form-group">
                <label>Price</label>
                <input type="text" class="form-control" aria-label="nom Auteur" name="price">
              </div>
              <div class="form-group">
                <label>ID Fournisseur</label>
                <input type="text" class="form-control" aria-label="nom Auteur" name="id_fournisseur">
              </div>
              <div class="form-group">
                <label>ID Rayon</label>
                <input type="text" class="form-control" aria-label="nom Auteur" name="id_rayon">
              </div>
  
              <button type="submit" class="btn btn-primary btn-block">Submit</button>
            </form>
          </section>
        </section>
      </section>
      
    </div>
 

  </div>



















